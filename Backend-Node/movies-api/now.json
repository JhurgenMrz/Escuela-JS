{
    "name": "platzivideo",
    "version": 2,
    "builds": [{
        "src": "index.js",
        "use": "@now/node"
        }],
    "routes":[{
        "src": "/(.*)",
        "dest": "/index.js"
    }],
    "env":{
        "DB_USER": "@platzimovie-db-user",
        "DB_PASSWORD": "@platzimovie-db-password",
        "DB_HOST": "@platzimovie-db-host",
        "DB_NAME": "@platzimovie-db-name"
    }
}